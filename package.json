{
  "name": "packformat-helper",
  "displayName": "Packformat Helper",
  "publisher":"ibfjmy25",
  "description": "Easily update pack.mcmeta pack_format for Minecraft datapacks.",
  "version": "1.0.5",
  "icon":"icon.png",
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "packformat-helper.updatePackFormat",
        "title": "Update pack.mcmeta pack_format"
      }
    ],
    "keybindings": [
      {
        "command": "packformat-helper.updatePackFormat",
        "key": "ctrl+shift+a",
        "when": "resourceLangId == 'mcmeta' || resourceFilename =~ /pack\\.mcmeta$/"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.108.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  },
  "repository": {
  "type": "git",
  "url": "https://github.com/ib-pino/packformat-helper"
}
}
